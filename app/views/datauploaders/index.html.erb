<h1>Listing datauploaders</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @datauploaders.each do |datauploader| %>
      <tr>
        <td><%= link_to 'Show', datauploader %></td>
        <td><%= link_to 'Edit', edit_datauploader_path(datauploader) %></td>
        <td><%= link_to 'Destroy', datauploader, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Datauploader', new_datauploader_path %>
